@model WFTest1.Models.RegisterModel
@{
    ViewBag.Title = "Register";
}
 @Html.Partial("Menu") 
<div class="reg_form">



    

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary()

    
    <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">User Registration Form</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-8">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Basic Elements
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-8">
                                    <form role="form">
                                        <div class="form-group">
                                             <label>UserName</label>
                                             @Html.TextBoxFor(m =>m.UserName, new { @class="form-control", @placeholder="Enter UserName"})                                           
                                             
                                        </div>
                                        <div class="form-group">
                                            <label>Name</label>
                                            
                                            @Html.TextBoxFor(m => m.Name, new { @class="form-control", @placeholder="Enter Name"})    
                                         
                                        </div>
                                        <div class="form-group">
                                            <label>Contact</label>
                                           @Html.TextBoxFor(m => m.Contact, new { @class="form-control", @placeholder="Enter Contact"})    
                                      
                                             </div>
                                        
                                         <div class="form-group">
                                            <label>Email</label>
                                            
                                             @Html.TextBoxFor(m => m.Email, new { @class="form-control", @placeholder="Enter Email"})
                                        
                                         </div>

                                        <div class="form-group">
                                            <label>Role</label>
                                            
                                             @Html.DropDownList("RoleId", String.Empty)                                           
                                        @*@Html.ValidationMessageFor(model => model.RoleId)*@
                                        
                                        </div>

                                         <div class="form-group">
                                            <label>Password</label>
                                            
                                             @Html.PasswordFor(m => m.Password, new { @class="form-control" ,@placeholder="Enter Password"})
                                        
                                         </div>

                                        <div class="form-group">
                                            <label>Password</label>
                                            
                                             @Html.PasswordFor(m => m.ConfirmPassword, new { @class="form-control" ,@placeholder="Enter Password"})
                                      
                                             </div>
                                                                     
                                        <button type="submit" class="btn btn-primary">Submit</button>
                                      
                                    </form>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                               
                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>

    
}
    </div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
